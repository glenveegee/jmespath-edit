    "jest": "26.0.1",
    "jest-cli": "26.0.1",
    "puppeteer": "2.1.1"
  },
  "dependencies": {
    "@metrichor/jmespath": "^0.1.3",
    "@metrichor/jmespath-plus": "0.0.4",
